{
    "version": 2,
    "builds": [
        {
            "src": "api/web.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    
    "functions": {
          "api/web.js": {
            "includeFiles": "templates/**"
          },
          "bus_api.py": {
            "includeFiles": "templates/**"
          }
        }
}